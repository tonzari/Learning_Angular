{
  "version": "1.0",
  "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "jquery@3.6.0",
            "destination": "wwwroot/lib/jquery/",
            "files": [
                "jquery.js",
                "jquery.min.js",
                "jquery.min.map",
                "jquery.slim.js",
                "jquery.slim.min.js",
                "jquery.slim.min.map"
            ]
        },
        {
            "library": "jquery-validate@1.19.2",
            "destination": "wwwroot/lib/jquery-validate/"
        },
        {
            "library": "jquery-validation-unobtrusive@3.2.12",
            "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
        },
        {
            "provider": "unpkg",
            "library": "bootstrap@5.0.1",
            "destination": "wwwroot/lib/bootstrap/"
        },
        {
            "provider": "unpkg",
            "library": "@fortawesome/fontawesome-free@5.15.3",
            "destination": "wwwroot/lib/fortawesome/fontawesome-free/",
            "files": [
                "css/*",
                "webfonts/*"
            ]
        }
    ]
}